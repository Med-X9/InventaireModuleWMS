[run]
source = apps, project, config
omit = 
    */migrations/*
    */tests/*
    */test_*
    */venv/*
    */env/*
    manage.py
    */settings/*
    */wsgi.py
    */asgi.py
    */__pycache__/*
    */staticfiles/*
    */static/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
